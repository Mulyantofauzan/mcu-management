{
  "name": "mcu-management",
  "version": "1.0.0",
  "description": "Aplikasi Web Manajemen Medical Check Up (MCU)",
  "main": "index.html",
  "scripts": {
    "dev": "echo 'Run: cd mcu-management && npx tailwindcss -i ./css/input.css -o ./css/output.css --watch'",
    "build": "mkdir -p dist && cp index.html dist/ && cp env-config.js dist/ && cp sw.js dist/ && cp -r js dist/ && cp -r pages dist/ && cp -r css dist/ && (cp -r images dist/ 2>/dev/null || true) && cp -r assets dist/ && echo 'Build complete'",
    "build:css": "npx tailwindcss -i ./css/input.css -o ./css/output.css --minify",
    "build:dev": "npx tailwindcss -i ./css/input.css -o ./css/output.css"
  },
  "keywords": [
    "mcu",
    "medical",
    "checkup",
    "management"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.86.0",
    "chart.js": "^4.4.0",
    "chartjs-plugin-datalabels": "^2.2.0",
    "dexie": "^3.2.4",
    "dotenv": "^17.2.3",
    "pako": "^2.1.0"
  }
}
