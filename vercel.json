{
  "buildCommand": "cd mcu-management && npm run build",
  "outputDirectory": "mcu-management/dist",
  "env": {
    "GOOGLE_CREDENTIALS": "@GOOGLE_CREDENTIALS_JSON",
    "GOOGLE_DRIVE_ROOT_FOLDER_ID": "@VITE_GOOGLE_DRIVE_ROOT_FOLDER_ID",
    "SUPABASE_URL": "@VITE_SUPABASE_URL",
    "SUPABASE_SERVICE_ROLE_KEY": "@SUPABASE_SERVICE_ROLE_KEY"
  },
  "functions": {
    "api/uploadToGoogleDrive/index.js": {
      "maxDuration": 60,
      "memory": 1024
    },
    "api/compress-upload/index.js": {
      "maxDuration": 60,
      "memory": 1024
    },
    "api/get-mcu-files/index.js": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)\\.js$",
      "dest": "/$1.js"
    },
    {
      "src": "/(.*)\\.css$",
      "dest": "/$1.css"
    },
    {
      "src": "/(.*)\\.json$",
      "dest": "/$1.json"
    },
    {
      "src": "/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/pages/(.*)",
      "dest": "/pages/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
