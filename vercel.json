{
  "buildCommand": "cd mcu-management && npm run build",
  "outputDirectory": "mcu-management/dist",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)\\.js$",
      "dest": "/$1.js"
    },
    {
      "src": "/(.*)\\.css$",
      "dest": "/$1.css"
    },
    {
      "src": "/(.*)\\.json$",
      "dest": "/$1.json"
    },
    {
      "src": "/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/pages/(.*)",
      "dest": "/pages/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
