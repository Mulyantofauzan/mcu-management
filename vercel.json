{
  "buildCommand": "cd mcu-management && npm run build",
  "outputDirectory": "mcu-management/dist",
  "env": {
    "GOOGLE_CREDENTIALS": "@google_credentials",
    "GOOGLE_DRIVE_ROOT_FOLDER_ID": "@google_drive_root_folder_id",
    "SUPABASE_URL": "@supabase_url",
    "SUPABASE_SERVICE_ROLE_KEY": "@supabase_service_role_key"
  },
  "functions": {
    "api/uploadToGoogleDrive/index.js": {
      "maxDuration": 60,
      "memory": 1024
    },
    "api/compress-upload/index.js": {
      "maxDuration": 60,
      "memory": 1024
    },
    "api/get-mcu-files/index.js": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)\\.js$",
      "dest": "/$1.js"
    },
    {
      "src": "/(.*)\\.css$",
      "dest": "/$1.css"
    },
    {
      "src": "/(.*)\\.json$",
      "dest": "/$1.json"
    },
    {
      "src": "/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/pages/(.*)",
      "dest": "/pages/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
