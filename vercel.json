{
  "version": 2,
  "projectSettings": {
    "sourceFilesOutsideRootDirectory": true
  },
  "buildCommand": "npm run build",
  "outputDirectory": "mcu-management/dist",
  "env": {
    "NODE_ENV": "production"
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH"]
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "status": 200
    }
  ]
}
